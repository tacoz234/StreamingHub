{
  "manifest_version": 3,
  "name": "Universal Arrow Navigation",
  "version": "0.1.0",
  "description": "Navigate any website with arrow keys and Enter.",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "../icons/web_streaming.png"
  }
}